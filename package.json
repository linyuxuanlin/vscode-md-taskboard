{
  "name": "vscode-md-taskboard",
  "displayName": "vscode-md-taskboard",
  "description": "A markdown based local taskboard(kanban) extension.",
  "version": "0.0.1",
  "engines": {
      "vscode": "^1.58.0"
  },
  "categories": ["Other"],
  "activationEvents": [
      "onCommand:markdownKanban.showBoard"
  ],
  "main": "./out/extension.js",
  "contributes": {
      "commands": [
          {
              "command": "markdownKanban.showBoard",
              "title": "Show Markdown Kanban"
          }
      ]
  },
  "scripts": {
      "vscode:prepublish": "tsc -p ./",
      "compile": "tsc -watch -p ./",
      "postinstall": "node ./node_modules/vsce/out/npm/postinstall.js"
  },
  "devDependencies": {
      "typescript": "^4.0.3",
      "vscode": "^1.1.36",
      "eslint": "^7.10.0",
      "@types/node": "^12.11.7",
      "@types/vscode": "^1.58.0"
  },
  "dependencies": {
      "markdown-it": "^12.0.4"
  }
}
